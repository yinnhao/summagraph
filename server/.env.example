# Server Configuration
PORT=3001

# API Keys (if needed for baoyu-xhs-images skill)
# API_KEY=your_api_key_here

# Mock Switch
# Set to 'false' to use real generation (requires python dependencies)
# Set to 'true' (default) to use mock data
MOCK_GENERATION=true

# Supabase Configuration
# Get these from your Supabase project settings > API
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# PayPal Configuration
# Get these from PayPal Developer Dashboard > My Apps & Credentials
PAYPAL_CLIENT_ID=your-paypal-client-id
PAYPAL_CLIENT_SECRET=your-paypal-client-secret
PAYPAL_WEBHOOK_ID=your-paypal-webhook-id
PAYPAL_MODE=sandbox
# Frontend PayPal Client ID (same as PAYPAL_CLIENT_ID)
VITE_PAYPAL_CLIENT_ID=your-paypal-client-id
# Subscription Plan IDs (create in PayPal Dashboard > Subscriptions)
VITE_PAYPAL_PRO_PLAN_ID=P-xxxxx
VITE_PAYPAL_PREMIUM_PLAN_ID=P-xxxxx

